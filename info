new_files = set(f.name for f in self.new_folder.glob("*.[xX][lL][sS]*") if not f.name.startswith("~"))
prev_files = set(f.name for f in self.prev_folder.glob("*.[xX][lL][sS]*") if not f.name.startswith("~"))
template_files = set(f.name for f in self.template_folder.glob("*.[xX][lL][sS]*") if not f.name.startswith("~"))