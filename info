def ensure_nested_folder(parent, child):
    full_path = os.path.join(parent, child)
    if not os.path.exists(full_path):
        os.makedirs(full_path)
        print(f"Created folder: {full_path}")
    else:
        print(f"Folder already exists: {full_path}")

# Example usage
ensure_nested_folder("output", "regression_dashboards")