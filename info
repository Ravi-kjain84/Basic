  Component_name STRING,
  Deviations_table_name STRING,
  Environment STRING,
  Run_group STRING,
  Sub_run_group STRING,
  Site STRING,
  Test_dataset STRING,
  Base_dataset STRING,
  Deviation_table_type STRING,
  Status STRING,
  Total_Deviation_count INT64,
  User STRING,
  Job_ID STRING
);